	function sendreq(type,uid) {
	if(type=="message")
		mess='Eu te enviei uma mensagem,para visualizar se cadastrar no Sync.';
	if(type=="friend")
		mess='Eu te adicionei como amigo para aceitar se cadastrar no Sync.';
	
	  FB.ui({
		method: 'apprequests',
		message: mess,
		to: uid,
		data: 'senddata'
	  },
	   function(response) {
		 if (response) {
		   alert('Post was published.');
		 } else {
		   alert('Post was not published.');
		 }
	   }
	  
	  );
	}
	



  </script>

